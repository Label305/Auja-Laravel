{
    "name": "label305/auja-laravel",
    "type": "library",
    "description": "Library to setup Auja for a Laravel 4 or 5 application. Auja is an easy-to-use, easy-to-implement admin interface for managing your data.",
    "keywords": ["Auja", "interface", "laravel"],
    "homepage": "https://github.com/Label305/Auja-Laravel",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Niek Haarman",
            "email": "niek@label305.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "label305/auja": "3.0.0-alpha8",
        "laravel/framework": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
	"phpspec/phpspec": "~2.1",
        "hamcrest/hamcrest-php": "1.1.1",
        "mockery/mockery": "0.9.1",
        "behat/behat": "~2.5",
        "henrikbjorn/phpspec-code-coverage": "~0.2",
        "satooshi/php-coveralls": "~0.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Label305\\AujaLaravel\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "minimum-stability": "stable"
}
